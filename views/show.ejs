<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>table</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <table class="table table-bordered table-border text-center shadow mt-4">
            <tr>
                <th>user_id</th>
                <th>user_name:</th>
                <th>user_gmail</th>
                <th>user_password</th>
                <th>user_delete</th>
                <th>user_Edit</th>
            </tr>
            <% user.forEach(function(use){ %>
             <tr>
             <td><%= use.user_id %></td>
             <td><%= use.user_name %></td>
             <td><%= use.user_email %></td>
             <td><%= use.user_password %></td>
             <td><a href="/delete/<%= use.user_id %>" class="btn btn-danger btn-sm"> delete</a></td>
             <td><a href="/edit/<%= use.user_id %>" class="btn btn-success btn-sm">edit</a></td>
            </tr>
            <%  }) %>
        </table>
    </div>
</body>
</html>